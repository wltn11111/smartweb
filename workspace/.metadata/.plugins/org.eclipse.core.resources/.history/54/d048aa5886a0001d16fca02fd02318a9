
public class Rotto {
	public static void main(String[] args) {
		
		int [] arr = new int[45];
		
		for(int i=0; i < arr.length; i++) {
			arr[i]=i+1;
		}
		for(int i=0 ; i<200 ; i++) {
			int x = (int)((Math.random()*45));
			int y = (int)((Math.random()*45));
			
			int tmp = arr[x];
			arr[x] = arr[y];
			arr[y] = tmp;
			
		}
		for(int i=0; i < 7; i++) {
		System.out.print("7°³ : " + arr[i] + " ");
		}
	}
}
